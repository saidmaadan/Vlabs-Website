header
  position fixed
  top 0
  left 0
  z-index 999
  width 100%
  height 70px
  background-color #062F33

header img
  margin-left -140px

header span
  color #15A3B3
  font-size 1.7em
  font-weight bolder

.navbar-inverse 
  background-color #0E7480
  border-color #0E7480

.navbar-inverse .navbar-nav .home-icon span
  color #DB3B3B
  font-size 1.5em

.navbar-inverse .navbar-nav .home-icon span:hover
  color #E8DADA
  font-size 1.6em

.navbar-inverse .navbar-nav .more-icon span
  color #E6EAEB
  font-size 1.2em

.navbar-inverse .navbar-nav .more-icon span:hover
  color #000
  font-size 1.2em

.navbar
  margin-top 70px

.navbar-inverse .navbar-nav>.active>a, .navbar-inverse .navbar-nav>.active>a:focus, .navbar-inverse .navbar-nav>.active>a:hover
  color #fff
  background-color #062F33

.navbar-toggle
  border 0 solid #0E7480

.navbar-inverse .navbar-toggle
  border-color #0E7480

.navbar-inverse .navbar-toggle:hover
  background-color #0E7480

.navbar-inverse .navbar-toggle:focus
  background-color #DE240B

.navbar-inverse .navbar-nav > li > a
  color #000

.contact
  display inline
  float right
  padding-top 10px   
  padding 10px
  font-size 1.3em
  color: #15CFCF

.contact a
  color #15CFCF

.contact a:hover
	color #E64210

section
  margin-top 120px
  height 100%

footer
  width 100%
  height 100%
  background-color #D5DFE0
  color #050505
  margin-top 30px
  bottom 0
  left 0

article .main-wrap
  height 100%

.main
  width 100%
  height 470px

.main h2
	margin 60px 70px

.main img
	width 100%
	margin 10px 0 30px 0

.main-carousel
  width 100%
  background-color #0E7480
  height 100%

.main-content
  font-size 2em
  color #ADB7B8
  height 100%
  margin 20px 0 10px 0
  

.main-content p
  border 1px solid #0E7480
  background-color #4E5F61
  border-radius 20px
  padding 20px 10px


.main-content span
  color red

.tech-img img
  padding-top 30px
  width 100%

.labs-content
  margin auto
  text-align center

.labs-content p
  font-size 16px
  text-align left

.labs-content span
  color #0E7480
  
  
.labs-content h2
  color #0E7480
  padding-bottom 20px

.labs-content h2 span
  font-weight bolder
  font-size 1.4em

.works-content
  width 100%
  margin 40px 0 20px 0
  text-align center

.works-content h2
  color #0E7480
  padding-bottom 20px

.works-content h2 span
  color red
  
.works-content img
  width 100%
  margin-top 15px

.works-content a
  color red
  font-size 1.4em
  font-weight bolder
  padding-bottom 30px

.works-content a:hover
  color #0E7480
  text-decoration none

.works-content p
  padding-top 10px
  text-align left

.works
	background-color #3A869C
	color #fff

.works h4
	text-align center
	padding 20px

.works-tools
  width 100%
  margin 20px 0 50px 0
  text-align center

.works-tools img
  width 100%

.works-tools h1
  font-size 2.3em
  padding 0 0 30px 0

.works-tools p
  text-align left

.works-tools h4
  text-align left

.main-service
  width 100%
  height 400px
  text-align center

.main-service img
  width 100%

.main-service h1
  font-size 2.5em
  color red
  margin-top -200px
  font-weight bold

.main-service kbd
  background-color #0E7480

.main-tech
  width 100%
  height 100%
  background-color #0E7480

.main-tech img
   width 100%

.main-tech h4
  font-size 35px
  margin 10px 20px
  color #C0E8ED
  font-weight bolder

.main-tech h4 kbd
  background-color #000
  color #BA8772

.main-tech h4 kbd:hover
  color #ED2A0C

.main-tech-headlines
  margin 40px 0 0 0
  text-align center
  font-size 3.5em
  font-weight bold
  color #000

.main-tech-list
  display inline-block
  font-size 24px
  margin 20px 3px 1px 3px

.main-tech-list kbd
  color #0E7480
  background-color #C0CFCC
  padding 12px
  border-radius 5px

.main-tech-list kbd:hover
  background-color #0E7484
  color #fff

.nav-tabs > li > a
  margin-top 40px
  color red
  font-size 24px

.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus
  background-color #0E7480
  color #fff

.getin-content
  margin 20px 0

.getin-main a
  background #ddd
  display inline-block
  margin-right 5px
  min-width 150px
  outline none
  padding 10px 40px 10px
  position relative
  text-decoration none


.getin-main .active
  background #d9534f
  color #fff

.panel-primary 
  border-color #ddd

.panel-primary > .panel-heading
  color #fff
  background-color #1C3634
  border-color #1C3634

.panel-danger > .panel-heading 
  color #ddd
  background-color #0E7480
  border-color #0E7480

#map
  width 100%
  height 440px

.infoWindowContent
  font-size  14px !important
  border-top 1px solid #ccc
  padding-top 10px

.angular-google-map-container
  width 100%
  height 440px

.bottom
  background-color #0E7480
  width 100%
  height 100%
  padding 10px

.bottom h4
  font-size 1.5em

.bottom a
  font-size 1.3em
  color #D1CDCD
  background-color black
  padding 13px
  border-radius 20px

.bottom a:hover
  font-size 1.3em
  color #15A3B3
  text-decoration none
  background-color #363232


.bottom button
  background-color #062F33
  border-radius 15px

.bottom button:hover
  background-color #0D373B

.bottom-last
  background-color #062F33
  width 100%
  height 100%
  padding 15px
  text-align center
  color #15A3B3

.bottom-last h5
  font-size 1.3em

.hidden 
  display none

.message
  padding 15px
  margin 0

.vertical-middle
  margin-top 20px

.form-contact
  background-color #ddd
  border-radius 5px
  box-shadow 0 1px 1px #ccc
  color #404040
  font-size 16px
  padding 30px 20px

.form-contact legend
  color #404040
  font-size 42px
  font-family "cookie", "cursive"
  font-weight normal
  line-height 1

.form-getQuote
  background-color #C3DBDE
  border-radius 5px
  box-shadow 0 1px 1px #ccc
  color #404040
  font-size 16px
  padding 30px 20px

.form-getQuote legend
  color #404040
  font-size 42px
  font-family "cookie", "cursive"
  font-weight normal
  line-height 1
 


  
  





  
 